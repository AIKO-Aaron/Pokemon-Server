#SET GROUND 0
#SET GROUNDLAYER 0

#SET RUBYTILES 0
#SET GRASS_X 0
#SET GRASS_Y 0
#SET GRASS 0

VAR TEST 0

LOAD SPRITESHEET PATH "/ch/aiko/pokemon/textures/RubyTiles.png" TO RUBYTILES SWIDTH 16 SHEIGHT 16 
LOAD SPRITE FROM RUBYTILES ATX GRASS_X ATY GRASS_Y FLOOR
LOAD SPRITE FROM RUBYTILES ATX 4 ATY 2 AUTO TREE LEVEL 1

LOAD AREA FROM RUBYTILES X 39 Y 31 WIDTH 7 HEIGHT 8 LEVEL 1 VAR POKECENTER_1

MAP ID "POKECENTER_1 + 7 * 7 - 1" LEVEL 0 TIMES 8
MAP ID "POKECENTER_1 + 7 - 1" LEVEL 0 TIMES 7 STEP 7
MAP ID "POKECENTER_1 + 2 + 6 * 7" LEVEL 0 TIMES 2

// PRINT TEXT "Test variable (Grass sprite id): " VAR TEST_VAR TEXT ", VAR1: " VAR TREE NEWLINE 

WRITE TIMES 64 SPRITE TEST_VAR TO GROUNDLAYER USING TEST
WRITE TIMES "64*2" SPRITE TREE TO 1
WRITE TIMES "64 * 2" SPRITE TREE TO 1 FROM "62 * 64"
FILL X 0 Y 2 WIDTH 2 HEIGHT 60 SPRITE TREE TO 1
FILL X 62 Y 2 WIDTH 2 HEIGHT 60 SPRITE TREE TO 1

FILL X 5 Y 5 WIDTH 7 HEIGHT 8 SPRITE POKECENTER_1 TO 1 INCR

ADD ENTITY TELEPORTER X "8 * 32 - 16" Y "11 * 32" PATH "/ch/aiko/pokemon/level/center.layout" DX "8 * 32 - 16" DY "10 * 32"